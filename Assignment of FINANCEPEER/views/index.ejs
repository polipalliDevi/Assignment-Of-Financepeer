            <th><strong>Id</strong></th>
            <th><strong>Title</strong></th>
            <th><stron>Body</stron></strong></th>
        </tr>
        <%datas.forEach((user)=>{%>
            <tr>
                <td>
                    <%=user.userId%>
                </td>
                <td>
